{
    "name": "neilime/zf2-assets-bundle",
    "type": "library",
    "description": "Zend Framework 2 module that provides assets bundling / caching",
    "keywords": ["assets","bundle","cache","zf2", "css", "js", "less"],
    "homepage": "http://neilime.github.io/zf2-assets-bundle/",
    "license": "MIT",
    "authors": [
        {
            "name": "Neilime",
            "homepage": "https://github.com/neilime",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/neilime/zf2-assets-bundle/issues"
    },
    "repositories": [
        {
	        "type": "vcs",
	        "url": "http://github.com/neilime/lessphp"
	    }
	],
	"suggest": {
        "ext-gd2": "GD library to compress png files"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*@stable",
        "leafo/lessphp": "dev-master",
        "natxet/CssMin": "dev-master",
        "nick4fake/jsmin": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "AssetsBundle": "src"
        },
        "classmap" : ["Module.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}