{
    "name": "neilime/zf2-assets-bundle",
    "type": "library",
    "description": "Zend Framework 2 module that provides assets bundling / caching",
    "keywords": ["assets","bundle","cache","zf2", "css", "js", "less"],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Neilime",
            "homepage": "https://github.com/neilime",
            "role": "Developer"
        }
    ],
    "repositories": [{
        "type": "package",
        "package": {
            "version": "dev-master",
            "name": "nodge/lessphp",
            "source": {
                "url": "https://github.com/Nodge/lessphp.git",
                "type": "git",
                "reference": "master",
                "autoload": {"classmap": ["lessc.inc.php"]},
                "target-dir": "nodge/lessphp"
            }
        }
    }],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "nodge/lessphp": "dev-master",
        "natxet/CssMin": "dev-master",
        "nick4fake/jsmin": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "AssetsBundle": "src"
        },
        "classmap" : ["Module.php"]
    }
}